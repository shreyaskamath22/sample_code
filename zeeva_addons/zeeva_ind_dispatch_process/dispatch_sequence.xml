<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

		<record id="seq_auto_dispatch_code" model="ir.sequence.type">
            <field name="name">Dispatch Serial Number</field>
            <field name="code">shipment.dispatch</field>
        </record>

        <record id="seq_dispatch" model="ir.sequence">
            <field name="name">Dispatch Serial Number</field>
            <field name="code">shipment.dispatch</field>
            <field name="prefix">DSP/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
		
        <record id="seq_auto_waybill_code" model="ir.sequence.type">
            <field name="name">Waybill Serial Number</field>
            <field name="code">dispatch.through</field>
        </record>

        <record id="seq_waybill" model="ir.sequence">
            <field name="name">Waybill Serial Number</field>
            <field name="code">dispatch.through</field>
            <field name="prefix">ZWB</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>