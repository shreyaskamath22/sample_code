    <!-- ZEEVA Product Finished Search View  -->
        <record id="zeeva_product_finished_search_view" model="ir.ui.view">
            <field name="name">zeeva.product.finished.search.view</field>
            <field name="model">product.product</field>
            <field name="type">search</field>
            <field eval="17" name="priority"/>
            <field name="arch" type="xml">
                <search string=" Finished Product">
                    <filter string="Earphones" icon="terp-accessories-archiver" domain="[('zeeva_categ_seq','&gt;=',100),('zeeva_categ_seq','&lt;=',199)]"/>
                    <filter string="Headphones" icon="terp-accessories-archiver" domain="[('zeeva_categ_seq','&gt;=',200),('zeeva_categ_seq','&lt;=',299)]"/>
                    <filter string="Speakers" icon="terp-accessories-archiver" domain="[('zeeva_categ_seq','&gt;=',300),('zeeva_categ_seq','&lt;=',399)]"/>
                    <filter string="Other products" icon="terp-accessories-archiver" domain="[('zeeva_categ_seq','&gt;=',400),('zeeva_categ_seq','&lt;=',999)]"/>
<!--                     <filter string="Services" icon="terp-accessories-archiver" domain="[('zeeva_categ_seq','&gt;=',1000),('zeeva_categ_seq','&lt;=',1049)]"/> -->
                    <field name="default_code" string="Product code" />
                    <field name="name" string="Product name" />
                    <field name="zeeva_partner_id"/>
                    <field name="product_tmpl_id" string="Raw product"/>
                    <field name="categ_id" widget="selection" string="Specific category"/>
                    <group  expand='0' string='Group by...'>
