        </record>
        <record id="product.item0" model="product.pricelist.item">
            <field name="price_version_id" ref="product.ver0"/>
            <field name="base" ref="product.list_price"/>
            <field name="name">HKD Pricelist Line</field>
        </record>
        
        <record id="list1" model="product.pricelist">
            <field name="name">USD Pricelist</field>
            <field name="type">sale</field>
            <field name="currency_id" ref="base.USD"/>
        </record>
        <record id="ver1" model="product.pricelist.version">
            <field name="pricelist_id" ref="list1"/>
            <field name="name">USD Pricelist Version</field>
        </record>
        <record id="item1" model="product.pricelist.item">
            <field name="price_version_id" ref="ver1"/>
            <field name="base" ref="product.list_price"/>
            <field name="name">USD Pricelist Line</field>
        </record>
        
        <!-- Property -->
        <record forcecreate="True" id="product.decimal_product_uom" model="decimal.precision">
            <field name="name">Product Unit of Measure</field>
            <field name="digits" eval="0"/>
        </record>
        
