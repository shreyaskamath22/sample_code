        <record id="base.main_company" model="res.company">
            <field name="name">Zeeva International Limited</field>
            <field name="rml_header1">Plugged in</field>
            <field name="rml_header">
&lt;header&gt;
    &lt;pageTemplate&gt;
        &lt;frame id="first" x1="1.3cm" y1="1.5cm" height="25.5cm" width="19.0cm"/&gt;
         &lt;stylesheet&gt;
            &lt;!-- Set here the default font to use for all &lt;para&gt; tags --&gt;
            &lt;paraStyle name='Normal' fontName="DejaVu Sans"/&gt;
            &lt;paraStyle name="main_footer" fontSize="8.0" alignment="LEFT"/&gt;
            &lt;paraStyle name="main_header" fontSize="8.0" leading="10" alignment="LEFT" spaceBefore="0.0" spaceAfter="0.0"/&gt;
         &lt;/stylesheet&gt;
        &lt;pageGraphics&gt;
            &lt;!-- Set here the default font to use for all &lt;drawString&gt; tags --&gt;
            &lt;setFont name="DejaVu Sans" size="8"/&gt;
            &lt;!-- You Logo - Change X,Y,Width and Height --&gt;
            &lt;image x="1.3cm" y="27cm" width="18.7cm" &gt;[[ company.logo_header or removeParentNode('image') ]]&lt;/image&gt;
            &lt;fill color="black"/&gt;
            &lt;stroke color="black"/&gt;

            &lt;!-- page header --&gt;

            &lt;!-- left margin --&gt;
            &lt;rotate degrees="90"/&gt;
            &lt;fill color="grey"/&gt;
            &lt;drawString x="2.65cm" y="-0.4cm"&gt;Zeeva International Limited&lt;/drawString&gt;
            &lt;fill color="black"/&gt;
            &lt;rotate degrees="-90"/&gt;

            &lt;!--page bottom--&gt;
            &lt;lines&gt;1.3cm 1.35cm 20.3cm 1.35cm&lt;/lines&gt;
            &lt;place x="1.3cm" y="0cm" height="1.25cm" width="19.0cm"&gt;
                &lt;para style="main_footer"&gt;[[ company.rml_footer ]]&lt;/para&gt;
                &lt;para style="main_footer"&gt;Generated by [[ user.name ]] on [[ formatLang(time.strftime("%Y-%m-%d"), date=True) ]]&lt;/para&gt;
            &lt;/place&gt;
            &lt;drawCentredString x="19.4cm" y="1cm"&gt; Page: &lt;pageNumber/&gt; / &lt;/drawCentredString&gt;
            &lt;drawCentredString x="20.1cm" y="1cm"&gt;&lt;pageCount/&gt;&lt;/drawCentredString&gt;
            
        &lt;/pageGraphics&gt;
    &lt;/pageTemplate&gt;
&lt;/header&gt;
            </field>
            <field name="currency_id" ref="base.HKD"/>
        </record>
        
        <!-- Currencies -->
        <record id="rateHKD" model="res.currency.rate">
            <field name="rate">1.0</field>
            <field name="currency_id" ref="base.HKD"/>
            <field eval="time.strftime('%Y-01-01')" name="name"/>
        </record>
        
        <record id="rateUSD" model="res.currency.rate">
            <field name="rate">0.129</field>
            <field name="currency_id" ref="base.USD"/>
            <field eval="time.strftime('%Y-01-01')" name="name"/>
        </record>
        
        <!-- Language config  -->
        <record id="base.lang_en" model="res.lang">
